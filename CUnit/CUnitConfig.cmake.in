# - Config file for the CUnit package
# It defines the @main_target@ library and adds it to link libraries

get_filename_component(CUNIT_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

if(NOT TARGET @main_target@)
  # file generated by install(export ...)
  include("${CUNIT_CMAKE_DIR}/@PROJECT_NAME@Targets.cmake")
endif()
